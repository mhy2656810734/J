class Solution {
    public char findTheDifference(String s, String t) {
      char[] array1 = s.toCharArray();
      char[] array2 = t.toCharArray();
      char count = 0;
      for (int i = 0; i < array1.length; i++){
          count ^= array1[i];
      }
      for (int j = 0; j < array2.length; j++) {
          count ^= array2[j];
      }
      return count;
    }
}
