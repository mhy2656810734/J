// 双指针法:让fast和slow指向0下标处
// 让fast开始走，如果发现nums[fast] != val时，将此时fast的值先放到show位置处再让show++
// show的位置就是数组长度
class Solution {
    public int removeElement(int[] nums, int val) {
        int fast = 0;
        int slow = 0;
        for (;fast < nums.length;fast++) {
            if (nums[fast] != val) {
                if (fast != slow) {
                    nums[slow] = nums[fast];
                }
                slow++;
            }
        }
        return slow;
    }
}
