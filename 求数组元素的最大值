public class text {
    public static int maxNum(int[] arr){
        int max=0;
        for(int i=0;i<arr.length;i++){
           if(arr[i]>max){
               max=arr[i];
            }
       
        }
        return max;
    }
    /**
     * 数组拷贝
     */
    public static void main(String[] args) {
        int[] array={1,2,3,4,5,6,7};

        int max = maxNum(array);
        System.out.println(max);
    }

}
